{
  "name": "web-worlding",
  "version": "0.1.0",
  "description": "3D spaces explorable on the web.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/ts-npm-package-boilerplate.git"
  },
  "keywords": [
    "3D",
    "webgl",
    "three",
    "typescript"
  ],
  "author": "Evan Kirkiles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evankirkiles/web-worldiing/issues"
  },
  "homepage": "https://github.com/evankirkiles/web-worlding#readme",
  "devDependencies": {
    "@types/jest": "29.2.4",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "8.29.0",
    "eslint-plugin-jest": "27.1.6",
    "jest": "29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.191",
    "@types/three": "^0.146.0",
    "camera-controls": "^1.37.4",
    "cannon-es": "^0.20.0",
    "cannon-es-debugger": "^1.0.0",
    "lodash": "^4.17.21",
    "nipplejs": "^0.10.0",
    "three": "^0.147.0",
    "three-mesh-bvh": "^0.5.19",
    "three-stdlib": "^2.20.4"
  }
}
