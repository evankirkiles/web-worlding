{
  "name": "web-worlding",
  "version": "0.1.0",
  "description": "3D spaces explorable on the web.",
  "main": "dist/web-worlding.js",
  "jsnext:main": "dist/web-worlding.module.js",
  "module": "dist/web-worlding.module.js",
  "types": "dist/web-worlding.d.ts",
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "rollup --config && terser dist/web-worlding.js -o dist/web-worlding.min.js --comments '/^!/' && downlevel-dts . .",
    "lint": "eslint src --ext ts",
    "typedoc": "typedoc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evankirkiles/web-worlding.git"
  },
  "keywords": [
    "3D",
    "webgl",
    "three",
    "typescript"
  ],
  "author": "Evan Kirkiles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evankirkiles/web-worldiing/issues"
  },
  "homepage": "https://github.com/evankirkiles/web-worlding#readme",
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/jest": "29.2.4",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "babel": "^6.23.0",
    "downlevel-dts": "^0.11.0",
    "eslint": "8.29.0",
    "eslint-plugin-jest": "27.1.6",
    "jest": "29.3.1",
    "prettier": "2.8.1",
    "rollup": "^3.7.3",
    "terser": "^5.16.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@types/three": "^0.146.0",
    "camera-controls": "^1.37.4",
    "cannon-es": "^0.20.0",
    "cannon-es-debugger": "^1.0.0",
    "nipplejs": "^0.10.0",
    "three": "^0.147.0",
    "three-mesh-bvh": "^0.5.19",
    "three-stdlib": "^2.20.4"
  }
}
